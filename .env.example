# Google Gemini API
GEMINI_API_KEY=your_gemini_api_key_here

# API Authentication (comma-separated API keys)
API_KEYS=key1_example,key2_example,key3_example

# Environment
ENVIRONMENT=development

# Logging
LOG_LEVEL=info

# Caching
ENABLE_CACHE=true
CACHE_TTL=3600

# Database
DATABASE_URL=sqlite:///./backend/data/processed/interventions.db

# Vector Store
CHROMA_PERSIST_DIR=./backend/data/chroma_db
COLLECTION_NAME=road_safety_interventions

# Gemini Settings
GEMINI_FLASH_MODEL=gemini-1.5-flash
GEMINI_PRO_MODEL=gemini-1.5-pro
GEMINI_EMBEDDING_MODEL=text-embedding-004

# Search Settings
DEFAULT_SEARCH_STRATEGY=hybrid
MAX_RESULTS=5
RAG_TOP_K=10

# API Settings (for deployment)
API_URL=http://localhost:8000
PORT=8000

# Frontend Settings
FRONTEND_URL=http://localhost:8501
